<Application x:Class="PL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PL"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <local:NullToVisebilityConverter x:Key="nullToVisebilityConverter"/>
        <local:NotNullToVisebilityConverter x:Key="notNullToVisebilityConverter"/>
        <local:NullToBooleanConverter x:Key="nullToBooleanConverter"/>
        <local:BoolToVisebilityConverter x:Key="boolToVisebilityConverter"/>
        <local:NotBoolToVisebilityConverter x:Key="notBoolToVisebilityConverter"/>
    </Application.Resources>
</Application>
